.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "List")}", deliverables_path
      %li= link_to "#{t("web-app-theme.new", :default => "New")}", new_deliverable_path
      %li.active= link_to "#{t("web-app-theme.edit", :default => "Edit")}", edit_deliverable_path
  .content
    %h2.title
      = t("web-app-theme.edit", :default => "Edit")
      Deliverable
    .inner
      = form_for @deliverable, :url => deliverable_path(@deliverable), :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
