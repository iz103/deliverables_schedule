.block
  .secondary-navigation
    %ul.wat-cf
      %li.first= link_to "#{t("web-app-theme.list", :default => "Deliverables")}", deliverables_path
      %li.active= link_to "#{t("web-app-theme.new", :default => "New")}", new_deliverable_path
  .content
    %h2.title
      = t("web-app-theme.new", :default => "New")
      Deliverable
    .inner
      = form_for :deliverable, :url => deliverables_path, :html => { :class => :form } do |f|
        = render :partial => "form", :locals => {:f => f}
- content_for :sidebar, render(:partial => 'sidebar')
